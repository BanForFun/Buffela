{
  "name": "@buffela/parser",
  "version": "2.1.0",
  "description": "Parser for the buffela wire protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mts",
      "require": "./dist/index.js"
    },
    "./internal/Calf": {
      "types": "./types/Calf.d.ts"
    }
  },
  "files": [
    "dist",
    "types",
    "schemas",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node tools/buildSchemas.js && tsup index.js --format cjs,esm --clean --out-dir dist && tsc",
    "prepublishOnly": "cp ../../../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BanForFun/Buffela.git"
  },
  "author": "BanForFun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BanForFun/Buffela/issues"
  },
  "homepage": "https://github.com/BanForFun/Buffela#readme",
  "dependencies": {
    "ajv": "^8.17.1"
  }
}
