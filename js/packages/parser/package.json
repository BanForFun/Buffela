{
  "name": "@buffela/parser",
  "version": "2.4.2",
  "description": "Parser for the buffela data format schema",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "types",
    "schemas",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup index.js --format cjs,esm --clean --out-dir dist --dts",
    "prepublishOnly": "cp ../../../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BanForFun/Buffela.git"
  },
  "keywords": [
    "buffela",
    "protobuf",
    "wire",
    "data",
    "serialization",
    "format",
    "protocol",
    "binary",
    "schema",
    "parser"
  ],
  "author": "BanForFun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BanForFun/Buffela/issues"
  },
  "homepage": "https://github.com/BanForFun/Buffela#readme",
  "dependencies": {
    "ajv": "^8.17.1"
  }
}
