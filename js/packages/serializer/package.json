{
  "name": "@buffela/serializer",
  "version": "3.0.0",
  "description": "Serializer for the buffela data format",
  "type": "module",
  "types": "types/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "types/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup index.js --format cjs,esm --clean --out-dir dist",
    "prepublishOnly": "cp ../../../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BanForFun/Buffela.git"
  },
  "keywords": [
    "buffela",
    "kotlin",
    "wire",
    "data",
    "serialization",
    "format",
    "protocol",
    "schema",
    "binary"
  ],
  "author": "BanForFun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BanForFun/Buffela/issues"
  },
  "homepage": "https://github.com/BanForFun/Buffela#readme",
  "dependencies": {
    "smart-buffer": "^4.2.0",
    "@buffela/parser": "3.0.0"
  }
}
