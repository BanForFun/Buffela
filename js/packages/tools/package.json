{
  "name": "@buffela/tools",
  "version": "2.4.2",
  "description": "Javascript/Typescript developer tools for the buffela data format",
  "bin": {
    "buffela-to-types": "dist/buffela-to-types.js",
    "buffela-to-json": "dist/buffela-to-json.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup buffela-to-types.js buffela-to-json.js --format cjs --clean --out-dir dist",
    "prepublishOnly": "cp ../../../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BanForFun/Buffela.git"
  },
  "keywords": [
    "buffela",
    "protobuf",
    "wire",
    "data",
    "format",
    "serialization",
    "protocol",
    "binary",
    "schema",
    "types",
    "tools"
  ],
  "author": "BanForFun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BanForFun/Buffela/issues"
  },
  "homepage": "https://github.com/BanForFun/Buffela#readme",
  "dependencies": {
    "@buffela/parser": "2.4.2",
    "@buffela/tools-common": "2.4.2"
  }
}
